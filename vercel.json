{
  "version": 2,
  "builds": [
    {
      "src": "Assignment_5/server.js",
      "use": "@vercel/node",
      "config": { 
        "includeFiles": [
          "Assignment_5/modules/**", 
          "Assignment_5/Data/**", 
          "Assignment_5/public/css/**", 
          "Assignment_5/views/**"
        ] 
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "Assignment_5/server.js"
    }
  ]
}
